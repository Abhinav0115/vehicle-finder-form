{
    "name": "vehicle-finder-backend",
    "version": "1.0.0",
    "description": "Backend API for Vehicle Rental System",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "db:migrate": "npx prisma migrate dev",
        "db:generate": "npx prisma generate",
        "db:seed": "node scripts/seed.js",
        "db:reset": "npx prisma migrate reset --force",
        "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed"
    },
    "keywords": [
        "vehicle",
        "rental",
        "booking"
    ],
    "author": "Abhinav Anand",
    "repository": {
        "type": "git",
        "url": "https://github.com/Abhinav0115/vehicle-finder-form/tree/main/backend"
    },
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.0.0",
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express-validator": "^7.0.1",
        "date-fns": "^2.30.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "prisma": "^5.0.0"
    }
}
